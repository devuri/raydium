{
    "name": "devuri/wpframework",
    "type": "project",
    "license": "MIT",
    "description": "Effortlessly spin up a fully-configured WordPress site in no time, specifically engineered for rapid development and deployment of secure, scalable WordPress applications.",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "devuri/wp-framework": "^5.0",
        "devuri/advanced-custom-site-status": "*",
        "wpackagist-plugin/application-passwords-manager": "*",
        "wpackagist-plugin/sim-clickable-links": "*",
        "wpackagist-plugin/disable-comments": "*",
        "wpackagist-plugin/disable-json-api": "*",
        "wpackagist-plugin/elementor": "*",
        "wpackagist-plugin/email-log": "*",
        "wpackagist-plugin/worker": "*",
        "wpackagist-plugin/query-monitor": "*",
        "wpackagist-plugin/slim-seo": "*",
        "wpackagist-plugin/stream": "*",
        "wpackagist-plugin/sucuri-scanner": "*",
        "wpackagist-plugin/wp-white-label-login": "*",
        "wpackagist-plugin/wp-auto-updates": "*",
        "wpackagist-theme/kadence": "*",
        "wpackagist-theme/hello-elementor": "*",
        "wpackagist-theme/twentytwentythree": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "extra": {
        "wordpress-install-dir": "public/wp",
        "installer-paths": {
            "public/mu-plugins/{$name}/": [
                "type:wordpress-muplugin"
            ],
            "public/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "public/app/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "johnpbloch/wordpress-core-installer": true,
            "composer/installers": true
        }
    }
}
